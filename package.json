{
  "name": "ccproxy",
  "version": "1.0.0",
  "description": "Claude Code subscription proxy server",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "ccproxy": "./src/cli.ts"
  },
  "scripts": {
    "start": "bun run src/cli.ts start",
    "login": "bun run src/cli.ts login",
    "dev": "bun run --watch src/cli.ts",
    "build": "bun run build:windows && bun run build:linux",
    "build:windows": "bun build --compile --target=bun-windows-x64 ./src/cli.ts --outfile=dist/ccproxy-windows-x64.exe",
    "build:linux": "bun build --compile --target=bun-linux-x64 ./src/cli.ts --outfile=dist/ccproxy-linux-x64"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.23",
    "@hono/node-server": "^1.19.9",
    "@openauthjs/openauth": "^0.4.3",
    "ai": "^6.0.50",
    "commander": "^14.0.2",
    "hono": "^4.11.6",
    "opencode-anthropic-auth": "^0.0.11"
  }
}
